{
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.7",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@esbuilder/html": "^0.0.6",
    "@open-wc/testing": "^3.0.0-next.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@web/dev-server": "^0.4.1",
    "@web/dev-server-rollup": "^0.6.1",
    "@web/test-runner": "^0.18.0",
    "eslint": "^8.56.0",
    "eslint-plugin-lit-a11y": "^1.1.0-next.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6"
  },
  "name": "memory-game",
  "description": "## Preparation",
  "type": "module",
  "version": "1.0.0",
  "main": "index.html",
  "scripts": {
    "build": "npm run importmaps && rimraf dist/rollup && rollup -c rollup.config.js",
    "importmaps": "jspm link src/index.dev.html -o src/index.html",
    "test": "web-test-runner src/**/*.test.js --node-resolve",
    "test:watch": "web-test-runner src/**/*.test.js --node-resolve --watch",
    "start": "web-dev-server --app-index src/index.dev.html --node-resolve --watch --open",
    "start:build": "npm run build && web-dev-server --root-dir dist/rollup --app-index src/index.html --compatibility none --open",
    "start:cdn": "npm run importmaps && web-dev-server --root-dir src --app-index index.dev.html --open --watch"
  },
  "author": "José María",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@lit-labs/router": "^0.1.3",
    "@lit/context": "^1.1.0",
    "@web/rollup-plugin-html": "^2.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "lit": "^3.1.1",
    "relit": "^2.4.1"
  }
}
